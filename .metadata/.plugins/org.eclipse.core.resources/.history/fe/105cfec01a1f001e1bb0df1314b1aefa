package practice;

import java.util.ArrayList;
import java.util.Collections;
import java.util.Comparator;
import java.util.List;

public class employee {
	public static void main(String[] args) {
		List<hello> l =new ArrayList<>();
		l.add(new hello("raj",3));
		l.add(new hello("ram",1));
		l.add(new hello("raj",8));
		l.add(new hello("raj",2));
		l.add(new hello("raj",0));
		Collections.sort(l,new hello());
	}

}
class hello implements Comparator<hello>
{
	String name;
	int id;
	hello(String name,int id)
	{
		this.name=name;
		this.id=id;
	}

	@Override
	public int compare(hello o1, hello o2) {
		
		return o1.id-o2.id;
	}

	@Override
	public String toString() {
		return "hello [name=" + name + ", id=" + id + "]";
	}
	
}

class IdBased implements Comparator<Integer>
{

	@Override
	public int compare(Integer o1, Integer o2) {
		
		return o1-o2;
	}

	
	
}