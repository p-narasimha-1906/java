package in.exception;

import java.util.Arrays;
import java.util.Scanner;


public class hello {
	int id;
	int age;
	hello(int id,int age)
	{
		this.id=id;
		this.age=age;
	}
	
	@Override
	public String toString() {
		return "hello [id=" + id + ", age=" + age + "]";
	}

	public static void main(String[] args) throws Exception {
//		Scanner sc = new Scanner(System.in);
//		System.out.println("enter string");
//		String s=sc.();
	
		hello h=new hello(1,12);
		hello h1=h;
		hello h2=h1;
		
		
		System.out.println("h clone:  "+h.hashCode());
		System.out.println("h1 clone:  "+h1.hashCode());
		System.out.println("h2 clone:  "+h2.hashCode());
	}
}


