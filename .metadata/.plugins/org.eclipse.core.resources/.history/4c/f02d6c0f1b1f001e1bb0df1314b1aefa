package practice;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;

public class Example {
	public static void main(String[] args) {
		List<boy> l=new ArrayList<>();
		l.add(new boy(2, "narasimha"));
		l.add(new boy(1, "narasimha"));
		Collections.sort(l);
		System.out.println(l);	
		}
	
	

}
class hello implements Comparable<boy>
{
	int id;
	String name;
	hello(int id,String name)
	{
		this.id=id;
		this.name=name;
	}

	@Override
	public int compareTo(boy o) {
		
		return this.id-o.id;
	}

	@Override
	public String toString() {
		return "hello [id=" + id + ", name=" + name + "]";
	}
	
	
}
